# Secret key for securing the application
SECRET_KEY=your_secret_key_here

# Debug mode
DEBUG=True or False

# Application configuration
APP_SETTINGS=development or production or testing or anything

# Database connection URL
DATABASE_URL=postgresql://<username>:<password>@<host>:<port>/<db_name>

# SQLAlchemy URI for connecting to the database
SQLALCHEMY_DATABASE_URI=postgresql://<username>:<password>@<host>:<port>/<db_name>

# Flask application
FLASK_APP=app.py or anything

# Flask debug mode
FLASK_DEBUG=1 or 0

# Email settings
MAIL_SERVER=smtp.example.com or smtp.gmail.com or smtp.mail.ru or smtp.yandex.ru 
MAIL_PORT=587 or 465 or 2525 or 25 
MAIL_USE_TLS=True or False
MAIL_USE_SSL=False or True
MAIL_USERNAME=your_email@example.com 
MAIL_PASSWORD=your_email_password 
MAIL_DEFAULT_SENDER=your_email@example.com 

# Folder for file uploads
UPLOAD_FOLDER=/path/to/upload/folder 